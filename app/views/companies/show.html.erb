                <li data-target="#myCarousel" data-slide-to="1"></li>
                <li data-target="#myCarousel" data-slide-to="2"></li>
              </ol>
              <!-- Carousel items -->
              <div class="carousel-inner">
                <div class="active item">
                  <div align="center" class="slideshare">
                    <%= render "slideshare" %>
                  </div>
                </div>
                <div class="item">
                  <div align="center" class="slideshare">
                    <%= render "youtube" %>
